<row>
<cell alignment="center" valignment="top" usebox="none">
\begin_inset Text

\begin_layout Plain Layout
Description:
\end_layout

\end_inset
</cell>
<cell alignment="center" valignment="top" usebox="none">
\begin_inset Text

\begin_layout Plain Layout
Creates a TCB and install it in the ROSA kernel.
\end_layout

\end_inset
</cell>
</row>
<row>
<cell alignment="center" valignment="top" usebox="none">
\begin_inset Text

\begin_layout Plain Layout
Parameters:
\end_layout

\end_inset
</cell>
<cell alignment="center" valignment="top" usebox="none">
\begin_inset Text

\begin_layout Itemize
tcb * TCB - A pointer to the TCB block to be created.
\end_layout

\begin_layout Itemize
char * id - A identification for the TCB block of length NAMESIZE (default
 NAMESIZE = 4)
\end_layout

\begin_layout Itemize
void * taskFunction - A pointer to the function which are to be executed
 by the task.
\end_layout

\begin_layout Itemize
int * stack - A pointer to the task stack area.
\end_layout

\begin_layout Itemize
int stackSize - The maximum allowed stack for this task.
\end_layout

\end_inset
</cell>
</row>
<row>
<cell alignment="center" valignment="top" usebox="none">
\begin_inset Text

\begin_layout Plain Layout
Return value:
\end_layout

\end_inset
</cell>
<cell alignment="center" valignment="top" usebox="none">
\begin_inset Text

\begin_layout Plain Layout
tcb * to the added task.
\end_layout

\end_inset
</cell>
</row>
</lyxtabular>

\end_inset


\end_layout

