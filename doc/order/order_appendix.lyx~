\end_layout

\begin_layout Subsection
Brief API list
\end_layout

\begin_layout Standard
This is a list of the sysTick API for ROSA.
\end_layout

\begin_layout LyX-Code
int ROSA_sysTickGet(void); 
\end_layout

\begin_layout LyX-Code
void ROSA_sysTickWait(int ticks); 
\end_layout

\begin_layout LyX-Code
void ROSA_sysTickWaitUntil(int tick);
\end_layout

